<%= react_component("ChecksForm",
  check: @check,
  errors: @check.errors.full_messages,
  queries: Blazer::Query.named.order(:name).select("id, name").map { |q| {text: q.name, value: q.id} },
  check_type: @check.respond_to?(:check_type),
  invert: @check.respond_to?(:invert),
  schedule: @check.respond_to?(:schedule),
  check_schedules: Blazer.check_schedules
  ) %>
