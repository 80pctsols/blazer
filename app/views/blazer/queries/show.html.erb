<% blazer_title @query.name %>

<%= react_component("QueriesShow", {
    query: @query,
    variable_params: variable_params,
    editable: @query.editable?(blazer_user),
    success: @success,
    error: @error,
    sql_errors: @sql_errors,
    statement: @statement,
    adapter: Blazer.data_sources[@query.data_source].adapter,
    bind_vars: @bind_vars
  }) %>
